{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "js-cookie": "^3.0.1",
    "mobx": "^6.3.0",
    "mobx-react": "^7.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "concurrently --kill-others \"yarn preview\" \"yarn test\" \"yarn storybook\"",
    "preview": "set PORT=5015 && craco start",
    "build": "craco build",
    "serve": "serve -s build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6007 -s public --no-manager-cache",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@types/enzyme": "^3.10.10",
    "@types/react-router-dom": "^5.3.2",
    "autoprefixer": "9",
    "concurrently": "^6.4.0",
    "craco-alias": "^3.0.1",
    "enzyme": "^3.11.0",
    "postcss": "7",
    "serve": "^13.0.2"
  }
}
